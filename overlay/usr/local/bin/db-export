#!/usr/bin/with-contenv bash

set -euo pipefail


#======================================================================================================================
# Run /etc/db/export executable
#======================================================================================================================

if [ $# -eq 1 ] && [ -n "${1}" ] ; then
    s6-setuidgid mysql /etc/db/export "${1}"
else
    bcg-error "db-export: You must specify a database name."
fi
